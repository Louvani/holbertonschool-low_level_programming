CC = gcc
SRC = main.c holberton.c
OBJ = main.o holberton.o
NAME = holberton
RM = rm -f
all : $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

main.o : main.c m.h
	gcc -c -o main.o main.c
holberton.o : holberton.c m.h
	gcc -c -o holberton.o holberton.c
.PHONY: clean
clean:
	$(RM) *~ $(NAME)
oclean:
	$(RM) $(OBJ)
fclean:
	$(RM) *~ $(NAME)
	$(RM) $(OBJ)
re:
	$(RM) $(OBJ)
	$(MAKE) all -f 3-Makefile